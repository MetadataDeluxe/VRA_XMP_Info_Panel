			private function updateComboBox(comboBox:XMPComboBox, xmpPath:String): void
			{
				//Get the value from the XMP
				var xmpVal:IXMPProperty = xmpAccess.getProperty(xmpPath);
				if(xmpVal && xmpVal.value)
				{
					var combovalObj:Object = {label: xmpVal.value, data: xmpVal.value};
					//Look through the combo box until we find the value, then select it
					for(var i:int = 0; i<comboBox.dataProvider.length; i++)
					{
						if(comboBox.dataProvider[i].data == combovalObj.data)
						{
							comboBox.selectedIndex = i;
							//comboBox.dispatchEvent(new ListEvent("change"));
							return;
						}
					}
				}
				else{
					comboBox.selectedIndex = 0;
				}
			}			
			
